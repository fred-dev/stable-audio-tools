{
  "model_type": "diffusion_cond",
  "sample_size": 524288,
  "sample_rate": 44100,
  "audio_channels": 1,
  "model": {
    "pretransform": {
      "type": "dac_pretrained",
      "config": {
        "model_bitrate": "16kbps",
        "quantize_on_decode": true,
        "scale": 5.3
      }
    },
    "conditioning": {
      "configs": [
        {
          "id": "latitude",
          "type": "number",
          "config": {
            "min_val": -54.617412,
            "max_val": -10.13994
          }
        },
        {
          "id": "longitude",
          "type": "number",
          "config": {
            "min_val": 96.8233,
            "max_val": 167.9619
          }
        },
        {
          "id": "temperature",
          "type": "number",
          "config": {
            "min_val": -3.83,
            "max_val": 42.16
          }
        },
        {
          "id": "humidity",
          "type": "number",
          "config": {
            "min_val": 1,
            "max_val": 100.0
          }
        },
        {
          "id": "wind_speed",
          "type": "number",
          "config": {
            "min_val": 0,
            "max_val": 28.61
          }
        },
        {
          "id": "pressure",
          "type": "number",
          "config": {
            "min_val": 974.0,
            "max_val": 1052.0
          }
        },
        {
          "id": "minutes_of_day",
          "type": "number",
          "config": {
            "min_val": 0,
            "max_val": 1439
          }
        },
        {
          "id": "day_of_year",
          "type": "number",
          "config": {
            "min_val": 1,
            "max_val": 366
          }
        }
      ],
      "cond_dim": 768
    },
    "diffusion": {
      "type": "adp_cfg_1d",
      "cross_attention_cond_ids": [
        "latitude",
        "longitude",
        "temperature",
        "humidity",
        "wind_speed",
        "pressure",
        "minutes_of_day",
        "day_of_year"
      ],
      "config": {
        "in_channels": 128,
        "context_embedding_features": 768,
        "context_embedding_max_length": 79,
        "channels": 128,
        "resnet_groups": 16,
        "kernel_multiplier_downsample": 2,
        "multipliers": [1, 2, 4, 8, 16],
        "factors": [1, 2, 2, 2],
        "num_blocks": [1, 2, 2, 2],
        "attentions": [0, 1, 1, 1, 1],
        "attention_heads": 8,
        "attention_multiplier": 2,
        "use_nearest_upsample": false,
        "use_skip_scale": true,
        "use_context_time": true
      }
    },
    "io_channels": 64
  },

  "training": {
    "learning_rate": 4e-5,
    "demo": {
      "demo_every": 2500,
      "demo_steps": 100,
      "num_demos": 3,
      "demo_cfg_scales": [3,6,9],
